{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "align": [false, "parameters"],
        "no-console": false,
        "no-shadowed-variable": false,
        "trailing-comma": [ false ],
        "object-literal-sort-keys": false,
        "variable-name": [
            "check-format",
            "allow-leading-underscore"
        ]
    },
    "jsRules": {
      "no-console": false
    },
    "rulesDirectory": []
}